<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.lucasfaria.viasacra">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_via_sacra"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_via_sacra"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".acitivity.SobreActivity"></activity>
        <activity android:name=".acitivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>